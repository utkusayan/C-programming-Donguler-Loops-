#include <stdio.h>
#include <stdlib.h>

int main()
{
    short int sayi;
    printf("Sayiyi girin:");
    scanf("%d",&sayi);
    int test=1;
    int i;
    if(sayi==1)
    {
        test=0;
    }
    else
    {
        for(i=sayi-1;i>1;i--)
            if(sayi%i==0)
        {
            test=0;
        }
    }
    if(test==0)
    {
        printf("Sayi asal degildir");
    }
    else
    {
        printf("Sayi asaldir");

    }

    return 0;
}
